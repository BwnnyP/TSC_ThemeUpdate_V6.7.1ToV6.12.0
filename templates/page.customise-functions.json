                    }
                },
                "richtext_GPypVx": {
                    "type": "richtext",
                    "settings": {
                        "content": "<h5>P2: Startup Mode Setting (Kick-to-Start or Zero-Start)<\/h5><ul><li><strong>Zero-Start<\/strong>: Set to <strong>0<\/strong> to allow the scooter to start from a standstill without kicking.<\/li><li><strong>Kick-to-Start<\/strong>: Set to <strong>1<\/strong> (default) to require the scooter to reach a speed above 3 km\/h before the motor engages.<\/li><\/ul>"
                    }
                }
            },
