            "settings": {
                "full_width": true,
                "remove_vertical_spacing": false,
                "remove_horizontal_spacing": false,
                "liquid": "<section id=\"stats\" class=\"sc-wrap\">\n  <style>\n    :root{\n      --brand:#e31919;\n      --brand-400:#f15a5a;\n      --card:#ffffff;\n      --shadow: 0 14px 35px rgba(0,0,0,.10), 0 2px 6px rgba(0,0,0,.04);\n      --shadow-soft: 0 10px 28px rgba(0,0,0,.08), 0 1px 4px rgba(0,0,0,.04);\n      --border: 1px solid rgba(0,0,0,.06);\n    }\n\n    .sc-wrap{\n      padding:40px 16px;       \/* smaller top\/bottom spacing *\/\n      background:#ffffff;\n    }\n\n    .sc-grid{\n      display:grid; gap:28px;\n      grid-template-columns:repeat(3,minmax(0,1fr));\n      max-width:950px;         \/* narrower overall grid *\/\n      margin:0 auto;\n    }\n    @media (max-width: 980px){ \n      .sc-grid{grid-template-columns:1fr; gap:20px; max-width:520px;} \n    }\n\n    .stat-card{\n      position:relative;\n      border-radius:18px;\n      background:\n        linear-gradient(180deg, rgba(241,90,90,.18), rgba(241,90,90,0) 60%),\n        var(--card);\n      box-shadow: var(--shadow-soft);\n      border: var(--border);\n      padding: 22px 20px;      \/* slightly smaller inside padding *\/\n      text-align:center;\n      transition: transform .35s ease, box-shadow .35s ease, border-color .35s ease, background .35s ease;\n    }\n    .stat-card:hover{\n      transform: translateY(-6px);\n      box-shadow: var(--shadow);\n      background:\n        linear-gradient(180deg, rgba(241,90,90,.22), rgba(241,90,90,0) 58%),\n        var(--card);\n      border-color: rgba(0,0,0,.08);\n    }\n\n    .number{\n      font-size: clamp(26px, 2.9vw, 40px);\n      font-weight: 800;\n      color: var(--brand);\n      margin: 2px 0 8px;\n      line-height: 1.1;\n      letter-spacing: .3px;\n      text-shadow: 0 1px 0 rgba(255,255,255,.5);\n    }\n    .label{\n      font-size: 15px;\n      color:#5c626a;\n    }\n  <\/style>\n\n  <div class=\"sc-grid\">\n    <div class=\"stat-card\">\n      <div class=\"number\" data-value=\"2000\">0<\/div>\n      <div class=\"label\">Valued Riders<\/div>\n    <\/div>\n    <div class=\"stat-card\">\n      <div class=\"number\" data-value=\"1000000\">0<\/div>\n      <div class=\"label\">km Logged<\/div>\n    <\/div>\n    <div class=\"stat-card\">\n      <div class=\"number\" data-value=\"500\">0<\/div>\n      <div class=\"label\">Five-Star Reviews<\/div>\n    <\/div>\n  <\/div>\n\n  <script>\n    (function(){\n      function animateCounter(el, duration){\n        var target = +el.getAttribute(\"data-value\");\n        var startTime = null;\n        function easeOutCubic(t){ return 1 - Math.pow(1 - t, 3); }\n        function step(ts){\n          if(!startTime) startTime = ts;\n          var p = Math.min((ts - startTime)\/duration, 1);\n          var eased = easeOutCubic(p);\n          var val = Math.floor(eased * target);\n\n          if (val >= 1000000){\n            el.textContent = (val\/1000000).toFixed(1).replace('.0','') + \"M+\";\n          } else if (val >= 1000){\n            el.textContent = Math.floor(val\/1000) + \"k+\";\n          } else {\n            el.textContent = val + \"+\";\n          }\n          if (p < 1) requestAnimationFrame(step);\n        }\n        requestAnimationFrame(step);\n      }\n\n      var section = document.querySelector('#stats');\n      var hasAnimated = false;\n      var obs = new IntersectionObserver(function(entries){\n        entries.forEach(function(entry){\n          if(entry.isIntersecting && !hasAnimated){\n            hasAnimated = true;\n            document.querySelectorAll('.number').forEach(function(el, i){\n              setTimeout(function(){ animateCounter(el, 2000); }, i*140);\n            });\n            obs.disconnect();\n          }\n        });\n      }, { threshold: 0.3 });\n      obs.observe(section);\n    })();\n  <\/script>\n<\/section>",
